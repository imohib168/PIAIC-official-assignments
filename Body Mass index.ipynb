   "source": [
    "print(\"Program to calculate body mass index: \")\n",
    "print()\n",
    "ht = float(input(\"Enter your height in metre please: \"))\n",
    "wt = float(input(\"Enter your weight in kilogram please: \"))\n",
    "print()\n",
    "cal = round(wt / (ht ** 2),2)\n",
    "print(\"Your Body Mass Index is: \",cal)"
